<h1>Create New Post</h1>

<h2>
  <%= form_for Post.new do |f| %>

    <%= f.label :title %><br>
    <%= f.text_field :title %>

    <br>

    <%= f.label :content %><br>
    <%= f.text_field :content %>

    <br>

    <%= f.hidden_field :created_at %>

    <br>

    Send To <%= f.label :user %>:<br>
    <%= f.collection_select :user_id, User.all, :id, :user_name %>

    <br>

    <%= f.submit %>

  <% end %>
</h2>
